{
  "name": "kodeka-clone",
  "version": "1.0.0",
  "description": "Mobile-first flex-based clone of kodeka.digital using TailwindCSS",
  "scripts": {
    "build:css": "tailwindcss -i ./style.css -o ./output.css --watch",
    "dev": "NODE_ENV=development npm run build:css",
    "build": "NODE_ENV=production tailwindcss -i ./style.css -o ./output.css"
  },
  "devDependencies": {
    "@stagewise/toolbar": "^0.2.1",
    "autoprefixer": "^10.4.14",
    "postcss": "^8.4.21",
    "tailwindcss": "^3.4.1"
  },
  "dependencies": {
    "gsap": "^3.13.0"
  }
}
